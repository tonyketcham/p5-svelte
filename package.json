		"@typescript-eslint/parser": "^4.31.1",
		"@zerodevx/svelte-toast": "^0.6.3",
		"autoprefixer": "^10.4.2",
		"eslint": "^8.7.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"highlight.js": "^11.4.0",
		"highlightjs-svelte": "^1.0.6",
		"postcss": "^8.4.5",
