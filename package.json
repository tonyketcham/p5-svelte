		"@types/p5": "^1.4.2",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"@zerodevx/svelte-toast": "^0.9.0",
		"autoprefixer": "^10.4.2",
		"eslint": "^8.10.0",
		"eslint-config-prettier": "^8.4.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"highlight.js": "^11.4.0",
		"highlightjs-svelte": "^1.0.6",
